#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    std::cin >> t;

    while (t--)
    {
        int n;
        std::cin >> n;

        std::vector<std::string> num(n);
        for (int i = 0; i < n; i++)
        {
            std::cin >> num[i];
        }

        int one = 0, zero = 0;
        for (auto j : num)
        {
            for (int i = 0; i < j.size(); i++)
            {
                if (j[i] == '0')
                    zero++;
                else
                    one++;
            }
        }

        int count = 0;
        for (int i = 0; i < n; i++)
        {
            if (num[i].size() <= one)
            {
                one = one - num[i].size();
                count++;
            }
            else if (num[i].size() <= zero)
            {
                zero = zero - num[i].size();
                count++;
            }
        }

        std::cout << count << '\n';
    }
}