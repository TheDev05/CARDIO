#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    std::cin >> t;

    while (t--)
    {
        int n;
        std::cin >> n;

        std::vector<int> num(n), result;

        std::set<int> res;
        std::map<int, int> data;

        for (int i = 0; i < n; i++)
        {
            std::cin >> num[i];

            data[num[i]]++;
            res.insert(i);
        }

        if (n == 1 && num[0] == 1)
        {
            std::cout << "1\n";
            std::cout << "0\n";

            continue;
        }

        int inox = 0, total = data.size();
        for (int i = 0; i < n; i++)
        {
            if (data.count(*res.begin() - inox) == false)
            {
                result.push_back(*res.begin() - inox);
                // std::cout << *res.begin() << " ";

                inox = *res.begin();
            }

            data[num[i]]--;

            if (data[num[i]] == 0)
                data.erase(num[i]);

            res.erase(inox + num[i]);
        }

        if (inox == 0)
        {
            std::cout << "1\n";
            std::cout << total << '\n';

            continue;
        }

        result.push_back(abs(inox - *res.begin()));
        // std::cout << *res.begin() << " ";

        // result.push_back(total - inox);

        std::cout << result.size() << '\n';
        for (auto i : result)
            std::cout << i << " ";

        std::cout << '\n';
    }
}